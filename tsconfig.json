{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "ES6",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",                         
    "strict": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  }, "include": ["src/**/*", "swagger.ts"],
}